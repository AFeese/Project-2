{{!-- YES --}}
<ul>
    {{#each users}}
        {{#unless no}}
            {{>users/notification-block yes=true}}
        {{/unless}}
    {{/each}}
</ul>
{{!-- NO --}}
<ul>
    {{#each users}}
        {{#if yes}}
            {{>users/notification-block no=false}}
        {{/if}}
    {{/each}}
</ul>